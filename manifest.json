{
    "name": "Libremdb",
    "id": "libremdb",
    "packaging_format": 1,
    "description": {
        "en": "Free & open source IMDb front-end",
        "fr": "Frontal IMDb gratuit et open source"
    },
    "version": "2.2.2~ynh1",
    "url": "https://libremdb.iket.me/",
    "upstream": {
        "license": "AGPL-3.0",
        "website": "https://libremdb.iket.me/",
        "demo": "https://libremdb.iket.me/about",
        "code": "https://github.com/zyachel/libremdb"
    },
    "license": "AGPL-3.0",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
